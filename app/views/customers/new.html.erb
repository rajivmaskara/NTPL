 <b>Add Customer details</b>

<%= form_for @customer do |f| %>
	
	<%= f.label :CustomerName %><br/>
	<%= f.text_field :customer_name %><br/>
	
	<%= f.label :PhoneNo %><br/>
	<%= f.text_field :phone_no %><br/>

	<%= f.label :Location %><br/>
	<%= f.text_field :location %><br/>

	<%= f.submit %>
<%end%>